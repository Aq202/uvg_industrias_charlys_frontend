{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "semi": "off",
        "no-unused-vars": "off",
        "react/function-component-definition": [
            2,
            {
              "namedComponents": ["arrow-function","function-declaration"],
              "unnamedComponents": "arrow-function"
            }
          ]
    }
}
